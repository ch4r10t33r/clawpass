{
  "name": "@ch4r10t33r/clawpass",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ch4r10t33r/clawpass.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "description": "ERC-8004 interface module for AI agent identity, reputation, and validation on blockchain",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "openclaw": {
    "extensions": ["./openclaw-plugin.ts"]
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "erc-8004",
    "ethereum",
    "ai-agents",
    "identity",
    "reputation",
    "validation",
    "blockchain",
    "web3",
    "moltbook",
    "moltx",
    "openclaw",
    "openclaw-plugin"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "ethers": "^6.11.1",
    "zod": "^3.22.4"
  },
  "peerDependencies": {
    "ethers": "^6.0.0"
  }
}
